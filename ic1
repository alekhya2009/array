#include <stdio.h>

int countNotEqualToKey(int array[], int size, int key) {
    int count = 0;
    for (int i = 0; i < size; i++) {
        if (array[i] != key) {
            count++;
        }
    }
    return count;
}

int main() {
    int array[] = {1, 2, 3, 4, 2, 5, 2, 6};
    int size = sizeof(array) / sizeof(array[0]);
    int key = 2;
    
    int result = countNotEqualToKey(array, size, key);
    
    printf("Number of elements not equal to %d: %d\n", key, result);
    
    return 0;
}
